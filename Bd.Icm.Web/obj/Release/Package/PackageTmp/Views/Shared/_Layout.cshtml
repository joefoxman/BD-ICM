@using Bd.Icm.Web.Helper
@{
    var version = ExtensionMethods.GetVersion();
}
<!DOCTYPE html>
<html lang="en" ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Becton Dickinson | Instrument Configuration Management</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script type="text/javascript" src="http://use.typekit.net/rrn8jpr.js"></script>
    <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
</head>
<body>
    @RenderBody()
    <h5 id="version" class="container-fluid"><small>Version @version</small></h5>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/plugins")
    @Scripts.Render("~/bundles/app")
</body>
</html>
